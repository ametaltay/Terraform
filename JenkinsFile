pipeline {
    agent any
    environment {
        AWS_ACCESS_KEY_ID     = credentials('awsID')
        AWS_SECRET_ACCESS_KEY = credentials('awspws')
        AWS_DEFAULT_REGION    =  "us-east-1"
    }


    stages {
        stage('terraform init') {
            steps {
                sh 'terraform init' 
                sh ' terraform plan'
            }
        }
    }
}
